 /*@ files=ProdScheduleMapper.xml namespace=com.paic.bs.afo.dao.mapper.ProdScheduleMapper id=listProdScheduByProdScheduCode  @*/
        select
        ID, CREATE_DATE, CREATE_BY, UPDATE_DATE, UPDATE_BY, IS_DELETE, PROD_LINE_CODE, PROD_BU,
        PROD_SERIES_NAME, PROD_CODE, PROD_NAME, PROD_SCHEDU_CODE, FIRST_LEVEL_TYPE, SECONDE_LEVEL_TYPE,
        THIRD_LEVEL_TYPE, AMOUNT, CURRENT_SCHEDU_STATUS, CURRENT_STATION, SCHEDUL_ONSHEL_DATE_START,
        SCHEDUL_ONSHEL_DATE_END,
        CHANNEL_CODE, GROUP_CODE, ORG_CODE, REGISTE_USER, LAST_CONSEN_TIME, ASSET_SCHEDUL_ID,
        BUS_UPDATE_TIME,
        BUS_UPDATE_USER,ADVANCE_NOTICE_STATUS,BATCH_NO,ACTION_EVENT,CURRENT_OPERATER,NEXT_OPERATER,SOURCE,ADVANCE_NOTES,
        SELL_START_DATE_TIME,SELL_END_DATE_TIME,GROUP_DES, OPEN_START_DATETIME, OPEN_END_DATETIME,
        RELATED_STATUS,RELATED_PRODID,SYN_STATUS
        from afo_prod_schedule
        where
        IS_DELETE = '0'
        AND (PROD_LINE_CODE = 'BROKER_SMALLCOL' OR PROD_LINE_CODE = 'PE')
        AND PROD_SCHEDU_CODE LIKE CONCAT('%', :P_prodScheduCode, '%')
        AND CURRENT_SCHEDU_STATUS IN ('2','3','4')
    